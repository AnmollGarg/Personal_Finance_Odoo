<odoo>
    <record id="seq_personal_finance_transaction" model="ir.sequence">
        <field name="name"> Personal Finance Sequence</field>
        <field name="code">personal.finance</field>
        <field name="prefix">TRANS</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_personal_finance_investment" model="ir.sequence">
        <field name="name"> Personal Finance Investment Sequence</field>
        <field name="code">personal.finance.investment</field>
        <field name="prefix">INV</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>
    
    <record id="seq_personal_finance_asset" model="ir.sequence">
        <field name="name"> Personal Finance Asset Sequence</field>
        <field name="code">personal.finance.asset</field>
        <field name="prefix">ASSET</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>
    
    <record id="seq_personal_finance_liability" model="ir.sequence">
        <field name="name"> Personal Finance Liability Sequence</field>
        <field name="code">personal.finance.liability</field>
        <field name="prefix">LIAB</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>

    <record id="seq_personal_finance_debt" model="ir.sequence">
        <field name="name"> Personal Finance Debt Sequence</field>
        <field name="code">personal.finance.debt</field>
        <field name="prefix">DEBT</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
    </record>
</odoo>